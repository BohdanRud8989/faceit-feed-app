{
  "name": "faceit-feed-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix .",
    "format": "prettier --write .",
    "test": "vitest",
    "prepare": "husky"
